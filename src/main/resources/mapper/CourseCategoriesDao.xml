	<select id="selectListVO"
		resultType="com.entity.vo.CourseCategoriesVO" >
		SELECT * FROM course_categories  course_categories
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>

	<select id="selectVO"
		resultType="com.entity.vo.CourseCategoriesVO" >
		SELECT  course_categories.* FROM course_categories  course_categories
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.CourseCategoriesView" >

		SELECT  course_categories.* FROM course_categories  course_categories
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>

	<select id="selectView"
		resultType="com.entity.view.CourseCategoriesView" >
		SELECT * FROM course_categories  course_categories <where> 1=1 ${ew.sqlSegment}</where>
	</select>



