<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.ExamStudentDao">

    <resultMap id="examStudentMap" type="com.entity.ExamStudentEntity">
        <result property="id" column="id"/>
        <result property="examId" column="exam_id"/>
        <result property="studentId" column="student_"/>
    </resultMap>

    <!-- Vid 列表 -->
    <select id="selectListVO" resultType="com.entity.vo.ExamStudentVO">
        SELECT * FROM exam_student
        <where> 1=1 ${ew.sqlSegment}</where>
    </select>

    <!-- 单个 VO -->
    <select id="selectVO" resultType="com.entity.vo.ExamStudentVO">
        SELECT * FROM exam_student
        <where> 1=1 ${ew.sqlSegment}</where>
    </select>

    <!-- 分页 View 列表 -->
    <select id="selectListView" resultType="com.entity.view.ExamStudentView">
        SELECT * FROM exam_student
        <where> 1=1 ${ew.sqlSegment}</where>
    </select>

    <!-- 单个 View -->
    <select id="selectView" resultType="com.entity.view.ExamStudentView">
        SELECT * FROM exam_student
        <where> 1=1 ${ew.sqlSegment}</where>
    </select>

</mapper>
